<div>
    <h1>Hola, {{ user.name }}, Welcome to the Jungle!!</h1>

    {{#if isAdmin }}

        {{#each foods}}
            <div>
                <p>{{this.name}}</p>
                <p><b>{{this.price}}</b></p>
                <hr>
            </div>

        {{/each}}


    {{else }}
        <p>No tienes permisos para ver la lista</p>
    {{/if }}
</div>

<script src="/js/index.js"></script>

